    "dependencies": {
        "react": "16.8.6",
        "react-native": "0.60.4",
        "react-native-gesture-handler": "^1.3.0",
        "react-native-reanimated": "^1.1.0",
        "react-navigation": "^3.11.1",
        "react-redux": "^7.1.0",
        "redux": "^4.0.4",
        "redux-logger": "^3.0.6",
        "redux-persist": "^5.10.0",
        "redux-saga": "^1.0.5",
